{
  "name": "react-decorators",
  "version": "1.2.0",
  "description": "A collection of react decorators to enhance components capabilities",
  "main": "./index.js",
  "scripts": {
    "preversion": "npm test",
    "prepublish": "npm test",
    "unit-test": "mocha tests/",
    "test": "nyc mocha tests/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iMoses/react-decorators.git"
  },
  "keywords": [
    "react",
    "decorators",
    "classnames",
    "css-modules",
    "context",
    "inject",
    "utils"
  ],
  "author": "Ido Moshe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iMoses/react-decorators/issues"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ],
    "require": [
      "./tests/bootstrap"
    ],
    "exclude": [
      "tests"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "homepage": "https://github.com/iMoses/react-decorators#readme",
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "babel-plugin-istanbul": "^5.1.4",
    "babel-plugin-rewire": "^1.2.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "jsdom": "^15.1.1",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^14.1.1"
  }
}
