    "url": "git+https://github.com/iMoses/react-decorators.git"
  },
  "keywords": [
    "react",
    "decorators",
    "classnames",
    "css-modules",
    "context",
    "inject",
    "utils"
  ],
  "author": "Ido Moshe",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/iMoses/react-decorators/issues"
  },
  "nyc": {
    "reporter": [
      "lcov",
      "text"
